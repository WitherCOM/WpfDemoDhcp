<Window x:Class="DhcpProba.View.KliensUzenet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DhcpProba.View"
        mc:Ignorable="d"
        Title="KliensUzenet" Height="200" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Text="{Binding EditText}" TextWrapping="Wrap" VerticalAlignment="Center" Width="120"/>
        <Button x:Name="button" Content="Request" HorizontalAlignment="Left" Margin="244,154,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="Cancel" HorizontalAlignment="Left" Margin="10,154,0,0" VerticalAlignment="Top" Command="{Binding Cancel}"/>

    </Grid>
</Window>
